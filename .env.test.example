# Test Environment Variables
# Copy this file to .env.test and update with your test values

# Overseerr Configuration (use test instance or mock values)
OVERSEERR_URL=http://localhost:5055
OVERSEERR_API_KEY=test-api-key

# Discord Configuration (use test bot credentials)
DISCORD_TOKEN=test-discord-token
DISCORD_SERVER_ID=123456789012345678

# Optional: Set to 'true' to enable Discord commands in tests
ENABLE_SLASH_COMMANDS=false

# Optional: Set to 'true' to enable quota checking functionality
ENABLE_QUOTA_CHECK=true